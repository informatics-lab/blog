---
layout: 
---

<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }} - projects</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ "/projects.feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml" />
    {% for projects in site.projects}
      <item>
        <title>{{ project.title | xml_escape }}</title>
        <description>{{ project.content | xml_escape }}</description>
        <author>{{ project.author }}</author>
        <summary>{{ project.summary }}</summary>
        <categories>{{ project.project }}<categories>
        <thumbnail>{{ project.thumbnail }}</thumbnail>
      </item>
    {% endfor %}
  </channel>
</rss>
